<?xml version="1.0" encoding="utf-8"?>

<odoo>
 <data>
    <record id="res_config_settings_transport_view_form_bridge" model="ir.ui.view">
        <field name="name">res.config.settings.transport.view.form</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//setting[@id='delivery']" position="after">
                <setting id="stock_transport_download" help="Transport Management System">
                    <field name="module_stock_transport"/>
                </setting>
            </xpath>
        </field>
    </record>
</data>
</odoo>
